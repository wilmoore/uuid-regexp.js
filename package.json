  },
  "dependencies": {},
  "devDependencies": {
    "dependency-check": "^2.4.0",
    "fixpack": "^2.2.0",
    "istanbul": "^0.3.13",
    "nodemon": "^1.3.7",
    "standard": "^3.7.1",
    "tap-spec": "^2.2.2",
    "tape": "^4.0.0",
    "tape-catch": "^1.0.4"
  },
  "homepage": "https://github.com/wilmoore/uuid-regexp.js",
  "keywords": [
